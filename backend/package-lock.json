{
  "name": "ecommerce-backend",
  "version": "1.0.0",
  "description":"Backend para sistema de E-commerce con node.js,Expresss y Sequelize",
  "main": "server.js",
  "scripts":{
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db":"node config/createdatabase.js",
    "reset-db":"node config/createdatabase.js",
